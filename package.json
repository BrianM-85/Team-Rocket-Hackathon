{
  "name": "Team-Rocket-Hackathon",
  "description": "This project was generated from the Mintbean bulma-theme template using the mintbean-cli tool.",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "build": "rm -fr .cache dist && parcel build index.html --public-url '.'",
    "css-build": "node-sass --omit-source-map-url src/bulma-theme.scss src/bulma-theme.css",
    "css-watch": "npm run css-build -- --watch",
    "css-start": "npm run css-watch"
  },
  "dependencies": {
    "bulma": "^0.9.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "parcel": "^1.12.4"
  },
  "devDependencies": {
    "sass": "^1.26.10"
  }
}
